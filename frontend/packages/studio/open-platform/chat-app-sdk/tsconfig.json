{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@coze-arch/ts-config/tsconfig.web.json",
  "compilerOptions": {
    "rootDir": "./",
    "types": ["vitest/globals", "node"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*.json",
    "src",
    "rspack-config",
    "stories",
    "vitest.config.ts"
  ],
  "exclude": ["node_modules"]
}
